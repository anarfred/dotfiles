cd ~/Documents/masterthesis
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +1 abstract.tex
badd +0 acronymes.tex
badd +0 annexe.tex
