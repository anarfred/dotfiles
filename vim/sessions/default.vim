badd +0 chapitres/chapitre3/chapitre3.tex
badd +0 chapitres/chapitre4/chapitre4.tex
badd +0 chapitres/chapitre5/chapitre5.tex
argglobal
silent! argdel *
argadd chapitres/chapitre1/chapitre1.tex
argadd chapitres/chapitre2/chapitre2.tex
argadd chapitres/chapitre3/chapitre3.tex
argadd chapitres/chapitre4/chapitre4.tex
argadd chapitres/chapitre5/chapitre5.tex
edit chapitres/chapitre1/chapitre1.tex
set splitbelow splitright
set nosplitbelow
set nosplitright
wincmd t
set winheight=1 winwidth=1
